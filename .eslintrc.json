{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:vue/vue3-essential"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "indent": ["warn", 4],
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "no-tabs": "off",
        "no-plusplus": "off",
        "implicit-arrow-linebreak": "off",
        "operator-linebreak": "off",
        "no-confusing-arrow": "off",
        "function-paren-newline": "off",
        "vue/no-v-for-template-key": "off"
    }
}
